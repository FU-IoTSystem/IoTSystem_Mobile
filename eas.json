{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_COMMUNITY_AUTOLINKING": "0",
        "KOTLIN_VERSION": "2.0.21",
        "SKIP_WINDOWS_BUILD": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_USE_COMMUNITY_AUTOLINKING": "0",
        "KOTLIN_VERSION": "2.0.21",
        "SKIP_WINDOWS_BUILD": "true"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_USE_COMMUNITY_AUTOLINKING": "0",
        "KOTLIN_VERSION": "2.0.21",
        "SKIP_WINDOWS_BUILD": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
